  "scripts": {
    "seed": "node ./db/connections/seed-run.js",
    "test-app": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --runInBand",
    "test-jest": "npx jest features/propertiesFavourite/deletePropertiesFavourite.test.js"
  },
